CREATE TABLE public."Dataset Atenei"
(
    anno integer,
    codice character varying COLLATE pg_catalog."default",
    descrizione character varying COLLATE pg_catalog."default",
    code_un character varying COLLATE pg_catalog."default",
    cod_istat_comune character varying COLLATE pg_catalog."default",
    comune character varying COLLATE pg_catalog."default",
    id_provincia character varying COLLATE pg_catalog."default",
    provincia character varying COLLATE pg_catalog."default",
    id_regione character varying COLLATE pg_catalog."default",
    regione character varying COLLATE pg_catalog."default",
    id_regione_macro_istat character varying COLLATE pg_catalog."default",
    regione_macro_istat character varying COLLATE pg_catalog."default",
    numeratore numeric,
    denominatore numeric,
    indicatore numeric
);

CREATE TABLE public."Dataset RomaTre"
(
    interclasse integer,
    code_un character varying COLLATE pg_catalog."default",
    codicione character varying COLLATE pg_catalog."default",
    nome_corso character varying COLLATE pg_catalog."default",
    cod_classe character varying COLLATE pg_catalog."default",
    des_classe character varying COLLATE pg_catalog."default",
    id_comune character varying COLLATE pg_catalog."default",
    comune character varying COLLATE pg_catalog."default",
    id_regione_macro_istat character varying COLLATE pg_catalog."default",
    regione_macro_istat character varying COLLATE pg_catalog."default",
    anno integer,
    codice character varying COLLATE pg_catalog."default",
    descrizione character varying COLLATE pg_catalog."default",
    numeratore numeric,
    denominatore numeric,
    indicatore numeric,
    num_ateneo numeric,
    den_ateneo numeric,
    ind_ateneo numeric
);